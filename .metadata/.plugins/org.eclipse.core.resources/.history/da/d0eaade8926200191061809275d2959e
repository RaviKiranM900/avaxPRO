package com.avaxpro.serviceimpl;

import java.util.ArrayList;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Component;

import com.avaxpro.dao.DebtAndCreditRepository;
import com.avaxpro.entities.Employee;
import com.avaxpro.service.IPPortalEmployeeService;

/**
 * @author DELL
 *
 */
@Component
public class IPPortalEmployeeServiceImpl implements IPPortalEmployeeService{
	
	@Autowired
	private DebtAndCreditRepository ipPortalEmpRepo;

	@SuppressWarnings("unchecked")
	@Override
	public ArrayList<Employee> getAllEmployeeList() {
		// TODO Auto-generated method stub
		return (ArrayList<Employee>) ipPortalEmpRepo.findAll();
	}

}
